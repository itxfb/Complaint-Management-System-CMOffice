<script type="text/javascript">
    debugger;
    //sjcl.decrypt("password", sessionStorage.getItem('PreviousUrl'));
    if (sessionStorage.getItem('PreviousUrl') != null) {
        $('#PreviousUrl').val(sjcl.decrypt("password", sessionStorage.getItem('PreviousUrl')));
    }

    var dropDownDict = [];
    $(document).ready(function () {
        RegisterLocalValidation("#StatusChangeForm", "T1");
        OnComplaintNotificationSeen();

        $('form').removeData('validator');
        $('form').removeData('unobtrusiveValidation');
        $.validator.unobtrusive.parse('form');


        $(".collapse").collapse();
        $('#Filters3').collapse({ hide: true });

        $(".collapse").collapse();
        $('#FiltersShowEscalation').collapse({ hide: true });

        PopulateDropDownDictionary();
        RegisterDropDownListChangeEventsCustomDictionary(dropDownDict);
        

        //$(".collapse").collapse();
        $('#Filters4').collapse({ hide: false });

        @if (@Model.hasStatusHistory)
        {
            <text>
        ReloadDataForStatusChange('#StatusHistoryTable');
        </text>
        }


        @if (Model.hasTransferHistory)
        {
            <text>
        ReloadDataTableAbsent('#forwardToLogs2');
        $(".collapse").collapse();
        $('#Filters2').collapse({ hide: true })
        </text>


        }
        $('#statusList').trigger('change');
    });

    $("#StatusChangeForm").submit(function (event) {
        //event.preventDefault();
        debugger;
        setTimeout(function () {
            $("#CnicTxtBox-error").html("Cnic is required");
        });
        var elementsToPost = [];
        var isValid = ValidateFormOnSubmit("#StatusChangeForm", "T1", elementsToPost);
        if ($('#StatusChangeForm').valid() && isValid /*$('#AddComplaintForm').valid()*/) {
            ShowLoading();
            var response = SubmitForm("#StatusChangeForm", elementsToPost);
        }
        //return isValid;
        return false;
    });
    function OnStatusChangeCallback(data) {
        debugger;
        sessionStorage.removeItem('PreviousUrl');
        HideLoading();
    }

    function OnComplaintNotificationSeen() {
        @{
            if (Model.currDetailType == VmStakeholderComplaintDetail.DetailType.AssignedToMe)
            {
                BlNotification.SetUserNotifictionStatus((int)Model.Compaign_Id, Config.NotificationType.Complaint, Config.NotificationSubType.Launch, Model.ComplaintId, (int)Config.NotificationStatus.DontSend);
            }
        }
    }
    function PopulateDropDownDictionary() {
        dropDownDict.push({
            key: "userControlCategory",
            value: "ComplaintSubType"
        });

        dropDownDict.push({
            key: "userControlSubCategory",
            value: null,
        });

    }

    function OnCategoryChangeSubmit() {
        if ($('#CategoryChangeForm').valid()) {
            ShowLoading();
        }
    }

    function OnStatusChangeSubmit() {
        if ($('#StatusChangeForm').valid()) {
            ShowLoading();
        }
    }

    function OnStatusChangeComplete(data) {
        $('#PopupDiv').modal('hide');
        //$('.modal-content').Hide();
    }
    $('#statusList').on('change', function () {
        debugger;
        var value = this.value;
        var campaignId = $('#VmStatusChange_VmStatusChange_Compaign_Id').val();
        var isUserSDU = $('#isUserSDU').val();
        if(isUserSDU){
            if (value == 11) {
                $('#feedbackDiv').show(500);
            } else {
                $('#feedbackDiv').hide(1000);
            }
        }

    });


    function ReloadDataTableAbsent(divId) {
        $(divId).dataTable().fnDestroy();
        var table = $(divId).dataTable({
            "bSort": false,
            "ajax": "@Url.Action("GetComlaintsTransferedHistoryTable", "GeneralApi")?" + "complaintId=" + $('#ComplaintId').val(),
            "contentType": "application/json",
            "type": "GET",
            "bLengthChange": false,
            "columns": [
                { "data": "UserName" },
                { "data": "AssignedFromMedium" },
                { "data": "AssignedFromMediumValue" },
                { "data": "AssignedToMedium" },
                { "data": "AssignedToMediumValue" },
                { "data": "AssignedDate" },
                { "data": "Comment" }

            ],
        });
    }



    function OpenComplaintAttachments(id) {
        $('#PopupDiv2').empty();
        $('#PopupDiv2').load("../Complaint/FileViewer?complaintId=" + @Model.ComplaintId + "&attachmentRefType=" +@Convert.ToInt32(Config.AttachmentReferenceType.Add) +"&attachmentRefTypeId=" + id);
        $('#PopupDiv2').modal();
    }

</script>
