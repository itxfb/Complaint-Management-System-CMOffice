@using PITB.CMS_Common
@using PITB.CMS_Common.Models
@model PITB.CMS_Common.Models.View.Police.VmPolice.VmComplaintAction
<style>
    .Complaint.active {
        border-top-color: #fbcc3e !important;
    }

    .Suggestion.active {
        border-top-color: #008000 !important;
    }

    .Inquiry.active {
        border-top-color: #ff0000 !important;
    }

    label.normalFont {
        font-weight: normal;
    }

    .dynamicDivAdded {
        background: #e3f1cd; /*#efefee*/
        /*padding: 10px 0px; */
        padding-top: 25px;
        border-radius: 5px;
        border: 1px solid #c2c2c2;
    }

    .dynamicDiv {
        background: #f7f7f7; /*#efefee*/
        /*padding: 10px 0px; */
        padding-top: 25px;
        border-radius: 5px;
        border: 1px solid #c2c2c2;
    }
    /*.select2 {
        width: 100% !important;
    }*/
    /*input {
        text-transform: uppercase;
    }*/
</style>
<div id="ComplaintAction" class=" modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-body">
            @using (Html.BeginForm("OnSubmitComplaintAction", "Police", FormMethod.Post, new { id = "SubmitComplaintAction", enctype = "multipart/form-data", data_post_config = "method::ajax||type::post||callback::PostActionCallback||url::../Police/PostComplaintAction?complaintId=" + @Model.ComplaintId }))
            {
                @Html.AntiForgeryToken()
                <div class="box box-primary" style="box-shadow: none;">
                    <div class="box-header-blue with-border btn-header">

                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        <h4 class="modal-title">Reporting of marking authority [@Model.ComplaintIdStr]</h4>
                        @*<button class="pull-right btn btn-success" style="margin-right: 30px; margin-top: -25px" onclick=" PrintReport() ">Print</button>*@
                    </div>
                    <div class="box-body-white" style="margin-top: 5px;">
                        <div id="CallComplainantWithinTime">
                            <div class="form-group row">
                                <div class="col-md-12">
                                    <h5><u>1) Call (within 08 Hours)*</u></h5>
                                </div>
                            </div>
                            @{
                                if (Model.CurrentStep <= 0)
                                {
                                    <div id="Step0Editable" class="form-group row">
                                        <div class="col-md-12">
                                            <div class="col-md-6">
                                                <div class="col-md-6">
                                                    <label>Call to the complainant with in time </label>
                                                </div>
                                                <div class="col-md-6">
                                                    <input id="RadioCallWithin08Hours" name="RadioCallWithin08Hours" type="radio" class="hidden" data-val-tag="T1" @*data-val-is-required="True"*@ data-group-name="CallWithin08Hours" value="">
                                                    <label class="normalFont"><input type="radio" name="CallWithin08Hours" value="6223___Yes"> Yes </label>@*<br>*@
                                                    <label class="normalFont"><input type="radio" name="CallWithin08Hours" value="6224___No"> No </label>@*<br>*@
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please select Any Value" data-val-for-name="CallWithin08Hours">

                                                    </span>
                                                </div>
                                            </div>

                                            <div class="form-group col-md-6 dynamicDiv" style="display: none;" data-display-if="RadioCallWithin08Hours::6224___No">
                                                <div class="col-md-4">
                                                    <label>Give reason*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="text" data-val-tag="T1" class="form-control textDetail" name="CallWithin08HoursReason" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter detail" data-validation-for-type="text" data-val-for-name="CallWithin08HoursReason">

                                                    </span>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div id="Step0NonEditable" class="form-group row dynamicDivAdded">
                                        @{
                                            List<DbDynamicComplaintFields> listToShow = Model.ListDbDynamicComplaintFields.Where(n => n.Filter2 == 0).ToList();
                                            for (int i = 0; i < listToShow.Count; i++)
                                            {
                                                <div class="form-group col-md-6">
                                                    <div class="col-md-6">
                                                        <label>@listToShow[i].FieldName</label>
                                                    </div>
                                                    <div class="col-md-6">
                                                        @Html.DisplayFor(m => listToShow[i].FieldValue)
                                                    </div>
                                                </div>
                                            }
                                        }
                                    </div>
                                }
                            }
                        </div>

                        <div class="form-group row">
                            <div class="col-md-12">
                                <hr>
                            </div>
                        </div>
                    </div>


                    <div id="MeetingWithin48Hours" class="box-body-white" style="margin-top: 5px;display: none;" data-display-if="CurrentDivIndex::1,2,3,4,5" @*data-display-if="BtnMeetingWithin48Hours::Show"*@>
                        <div id="DivMeetingWithin48Hours">
                            <div class="form-group row">
                                <div class="col-md-12">
                                    <h5><u>2) Meeting (within 48 Hours)*</u></h5>
                                </div>
                            </div>
                            @{
                                if (Model.CurrentStep <= 1)
                                {
                                    <div id="Step1Editable" class="form-group row ">
                                        <div class="col-md-12">
                                            <div class="col-md-6">
                                                <div class="col-md-6">
                                                    <label>Complainant was called & meeting was made (heard in person) </label>
                                                </div>
                                                <div class="col-md-6">
                                                    <input id="RadioMeetingWithin24Hours" name="RadioMeetingWithin24Hours" type="radio" class="hidden" data-val-tag="T1" @*data-val-is-required="True"*@ data-group-name="MeetingWithin24Hours" value="">
                                                    <label class="normalFont"><input type="radio" name="MeetingWithin24Hours" value="6225___Yes"> Yes </label>@*<br>*@
                                                    <label class="normalFont"><input type="radio" name="MeetingWithin24Hours" value="6226___No"> No </label>@*<br>*@
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please select Any Value" data-val-for-name="MeetingWithin24Hours">

                                                    </span>
                                                </div>
                                            </div>

                                            <div class="form-group col-md-6 dynamicDiv" style="display: none;" data-display-if="RadioMeetingWithin24Hours::6226___No">
                                                <div class="col-md-4">
                                                    <label>Give reason*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="text" data-val-tag="T1" class="form-control textDetail" name="MeetingWithin24HoursReason" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter detail" data-validation-for-type="text" data-val-for-name="MeetingWithin24HoursReason">

                                                    </span>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div id="Step1NonEditable" class="form-group row dynamicDivAdded">
                                        @{
                                            List<DbDynamicComplaintFields> listToShow = Model.ListDbDynamicComplaintFields.Where(n => n.Filter2 == 1).ToList();
                                            for (int i = 0; i < listToShow.Count; i++)
                                            {
                                                <div class="form-group col-md-6">
                                                    <div class="col-md-6">
                                                        <label>@listToShow[i].FieldName</label>
                                                    </div>
                                                    <div class="col-md-6">
                                                        @Html.DisplayFor(m => listToShow[i].FieldValue)
                                                    </div>
                                                </div>
                                            }
                                        }
                                    </div>
                                }
                            }
                        </div>
                        @*<div class="form-group row">
                        <div class="col-md-8">

                        </div>
                        <div class="col-md-2">
                            <button type="button" id="BtnShowProceedingWithin48Hours" data-val-tag="T1" name="BtnProceedingWithin48Hours" value="Show" class="btn btn-primary pull-right"><span class="fa fa-eye"></span>&nbsp;Show Next</button>
                        </div>
                        <div class="col-md-2">
                            <button type="button" id="BtnHideProceedingWithin48Hours" data-val-tag="T1" name="BtnProceedingWithin48Hours" value="Hide" class="btn btn-primary pull-right"><span class="fa fa-eye-slash"></span>&nbsp;Hide Next</button>
                        </div>
                    </div>*@
                        <div class="form-group row">
                            <div class="col-md-12">
                                <hr>
                            </div>
                        </div>
                    </div>



                    <div class="box-body-white" style="margin-top: 5px;display: none;" data-display-if="CurrentDivIndex::2,3,4,5" @*data-display-if="BtnProceedingWithin48Hours::Show"*@>
                        <div id="BtnProceedingWithin48Hours" name="ProceedingWithin48Hours" data-clone-config="CloneName::ClonableDivProceeding||OnEventOfName::AddBtnProceeding||EventType::click">
                            <div class="form-group row">
                                <div class="col-md-6">
                                    <h5><u>3) Proceeding (within 48 Hours)*</u></h5>
                                </div>
                                <div class="col-md-1">
                                </div>
                                @{
                                    if (Model.CurrentStep <= 2)
                                    {
                                        <div class="col-md-5">
                                            <button id="BtnAddProceeding" type="button" name="AddBtnProceeding" class="btn btn-primary"><span class="fa fa-adn"></span>&nbsp;Add Proceeding</button>
                                            <button id="BtnRemoveProceeding" disabled="" type="button" name="BtnRemoveProceeding" class="btn btn-primary"><span class="fa fa-times"></span>&nbsp;Remove Proceeding</button>
                                        </div>
                                        @*<div class="col-md-">
                                    </div>*@
                                    }
                                }
                            </div>
                            @{
                                {
                                    List<DbPoliceActionReportLogs> listInterimLogs = Model.ListDbActionReportLogs.Where(n => n.Type == Config.PoliceActionReportType.Interim).ToList();

                                    if (listInterimLogs.Count > 0)
                                    {
                                        <div class="form-group row">
                                            <div class="col-md-12">
                                                <h6><u>i) previous proceedings</u></h6>
                                            </div>
                                        </div>
                                        <div id="InterimReportProceeding" class="form-group">
                                            @{ int count = 1; }
                                            @foreach (DbPoliceActionReportLogs dbPoliceActionReportLogs in listInterimLogs)
                                            {
                                                @*<span>"@count")</span>*@
                                                @*<span>@count) </span>*@
                                                <h6>@count) </h6>
                                                <div class="form-group dynamicDivAdded">
                                                    @*<h6>@count) </h6>*@
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">

                                                                    <label>Proceeding Date</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @Html.DisplayFor(m => dbPoliceActionReportLogs.ProceedingDateTime)
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">
                                                                    <label>Detail</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @Html.DisplayFor(m => dbPoliceActionReportLogs.Comments)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">
                                                                    <label>Entry Date</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @Html.DisplayFor(m => dbPoliceActionReportLogs.CreatedDateTime)
                                                                </div>
                                                            </div>

                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">
                                                                    <label>Attachment</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @if (dbPoliceActionReportLogs.DbAttachment != null)
                                                                    {
                                                                        string url = System.IO.Path.GetExtension(dbPoliceActionReportLogs.DbAttachment.Source_Url).ToLower();
                                                                        string source = (url == ".pdf" || url == ".docx" || url == ".csv" || url == ".xlsx" || url == ".xls")
                                                                            ? "/img/file.svg" : dbPoliceActionReportLogs.DbAttachment.Source_Url;

                                                                        <a target="_blank" href=@dbPoliceActionReportLogs.DbAttachment.Source_Url>
                                                                            <img width="100" height="100"
                                                                                 src=@source
                                                                                 alt="@dbPoliceActionReportLogs.DbAttachment.FileName">
                                                                            <label>@dbPoliceActionReportLogs.DbAttachment.FileName</label>
                                                                        </a>
                                                                    }
                                                                    else
                                                                    {
                                                                        @Html.Raw("N/A")
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                { count++; }
                                            }
                                        </div>
                                    }

                                }
                                if (Model.CurrentStep <= 2)
                                {
                                    <div id="ClonableDivProceeding" class="form-group row dynamicDiv proceedingInfo" style="display: none;" name="ClonableDivProceeding" @*data-clone-config="ChildOfName::MeetingWithin48Hours||OnEventOfName::AddBtnProceeding||EventType::click||NamesToReplicate::Proceeding_Date,Proceeding_Detail,Proceeding_Attachment,Proceeding_IsInterim"*@>
                                        <div class="col-md-12">
                                            <div class="form-group col-md-6">
                                                <div class="col-md-4">
                                                    <label>Date*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="text" data-val-tag="T1" class="form-control" readonly data-date-end-date="0d" data-input-format="Date" data-input-date-format="mm/dd/yyyy" name="Proceeding_Date" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter date" data-validation-for-type="text" data-val-for-name="Proceeding_Date">

                                                    </span>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6 ">
                                                <div class="col-md-4">
                                                    <label>Detail*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="text" data-val-tag="T1" class="form-control textDetail" name="Proceeding_Detail" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter detail" data-validation-for-type="text" data-val-for-name="Proceeding_Detail">

                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="form-group col-md-6 ">
                                                <div class="col-md-4">
                                                    <label>Please attach file</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="file" accept="image/*,.pdf,.csv,.docx,.xlsx,.xls" data-val-tag="T1" class="form-control" name="Proceeding_Attachment" @*data-val-is-required="True"*@ />
                                                    @*<span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter detail"  >

                                                </span>*@
                                                </div>
                                            </div>
                                            @*<div class="col-md-6">
                                            <div class="col-md-6">
                                                <label>Is Interim* </label>
                                            </div>
                                            <div class="col-md-6">
                                                <input id="Proceeding_IsInterim" name="RadioProceeding_IsInterim" type="radio" class="hidden" data-val-tag="T1" data-val-is-required="True" data-group-name="Proceeding_IsInterim" value="">
                                                <label class="normalFont"><input type="radio" name="Proceeding_IsInterim" value="6230___Yes"> Yes </label>
                                                <label class="normalFont"><input type="radio" name="Proceeding_IsInterim" value="6231___No"> No </label>
                                                <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please select Any Value" data-val-for-name="Proceeding_IsInterim">

                                                </span>
                                            </div>
                                        </div>*@

                                        </div>
                                    </div>
                                }
                            }
                        </div>
                        @*<div class="form-group row">
                        <div class="col-md-8">

                        </div>
                        <div class="col-md-2">
                            <button type="button" id="BtnShowFinalReport" data-val-tag="T1" name="BtnFinalReport" value="Show" class="btn btn-primary pull-right"><span class="fa fa-eye"></span>&nbsp;Show Next</button>
                        </div>
                        <div class="col-md-2">
                            <button type="button" id="BtnHideFinalReport" data-val-tag="T1" name="BtnFinalReport" value="Hide" class="btn btn-primary pull-right"><span class="fa fa-eye-slash"></span>&nbsp;Hide Next</button>
                        </div>
                    </div>*@
                        <div class="form-group row">
                            <div class="col-md-12">
                                <hr>
                            </div>
                        </div>
                    </div>

                    <div class="box-body-white" style="margin-top: 5px;display: none;" data-display-if="CurrentDivIndex::3,4,5" @*data-display-if="BtnFinalReport::Show"*@>
                        <div>
                            <div class="form-group row">
                                <div class="col-md-6">
                                    <h5><u>4) Final Report*</u></h5>
                                </div>
                            </div>
                            @{
                                {
                                    List<DbPoliceActionReportLogs> listFinalLogs = Model.ListDbActionReportLogs.Where(n => n.Type == Config.PoliceActionReportType.Final).ToList();

                                    if (listFinalLogs.Count > 0)
                                    {
                                        @*<div class="form-group row">
                                        <div class="col-md-12">
                                            <h6><u>i) previous proceedings</u></h6>
                                        </div>
                                    </div>*@
                                        <div id="InterimReportProceeding" class="form-group" style="word-break: break-word;">
                                            @*@{ int count = 1; }*@
                                            @foreach (DbPoliceActionReportLogs dbPoliceActionReportLogs in listFinalLogs)
                                            {
                                                @*<span>"@count")</span>*@
                                                @*<span>@count) </span>*@
                                                @*<h6>@count) </h6>*@
                                                <div class="form-group dynamicDivAdded">
                                                    @*<h6>@count) </h6>*@
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">

                                                                    <label>Proceeding Date</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @Html.DisplayFor(m => dbPoliceActionReportLogs.ProceedingDateTime)
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">
                                                                    <label>Detail</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @Html.DisplayFor(m => dbPoliceActionReportLogs.Comments)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">
                                                                    <label>Entry Date</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @Html.DisplayFor(m => dbPoliceActionReportLogs.CreatedDateTime)
                                                                </div>
                                                            </div>

                                                            <div class="form-group col-md-6">
                                                                <div class="col-md-6">
                                                                    <label>Attachment</label>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    @if (dbPoliceActionReportLogs.DbAttachment != null)
                                                                    {
                                                                        string url = System.IO.Path.GetExtension(dbPoliceActionReportLogs.DbAttachment.Source_Url).ToLower();
                                                                        string source = (url == ".pdf" || url == ".docx" || url == ".csv" || url == ".xlsx" || url == ".xls")
                                                                            ? "/img/file.svg" : dbPoliceActionReportLogs.DbAttachment.Source_Url;

                                                                        <a target="_blank" href=@dbPoliceActionReportLogs.DbAttachment.Source_Url>
                                                                            <img width="100" height="100"
                                                                                 src=@source
                                                                                 alt="@dbPoliceActionReportLogs.DbAttachment.FileName">
                                                                            <label>@dbPoliceActionReportLogs.DbAttachment.FileName</label>
                                                                        </a>
                                                                    }
                                                                    else
                                                                    {
                                                                        @Html.Raw("N/A")
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                //{ count++;}
                                            }
                                        </div>
                                    }

                                }
                                if (Model.CurrentStep <= 3)
                                {
                                    <div id="FinalReport" class="form-group row dynamicDiv">
                                        <div class="col-md-12">
                                            <div class="form-group col-md-6">
                                                <div class="col-md-4">
                                                    <label>Date*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="text" data-val-tag="T1" class="form-control" readonly data-input-format="Date" data-date-end-date="0d" data-input-date-format="mm/dd/yyyy" name="FinalReportDate" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter date" data-validation-for-type="text" data-val-for-name="FinalReportDate">

                                                    </span>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6 ">
                                                <div class="col-md-4">
                                                    <label>Detail*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="text" data-val-tag="T1" class="form-control textDetail" name="FinalReportDetail" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-is-required-message="Please enter detail" data-validation-for-type="text" data-val-for-name="FinalReportDetail">

                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="form-group col-md-6 ">
                                                <div class="col-md-4">
                                                    <label>Please attach file*</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <input type="file" accept="image/*,.pdf,.csv,.docx,.xlsx,.xls" data-val-tag="T1" class="form-control" name="FinalReportDetailAttachment" data-val-is-required="True" />
                                                    <span class="field-validation-valid label label-warning hidden" data-val-for-name="FinalReportDetailAttachment" data-val-is-required-message="Please add attachment">

                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            }
                        </div>

                        <div class="form-group row">
                            <div class="col-md-12">
                                <hr>
                            </div>
                        </div>
                    </div>

                    <div class="box-body-white" style="margin-top: 5px;display: none;" data-display-if="CurrentDivIndex::4,5" @*data-display-if="BtnFinalReport::Show"*@>
                        <div>
                            <div class="form-group row">
                                <div class="col-md-6">
                                    <h5><u>5) Disposal*</u></h5>
                                </div>
                            </div>
                            @{
                                if (Model.CurrentStep <= 4)
                                {
                                    <div id="DisposalForm">
                                        <div id="FinalReport" class="form-group row">
                                            <div class="col-md-12">
                                                <div class="form-group col-md-6">
                                                    <div class="col-md-4">
                                                        <label>Disposal Category*</label>
                                                    </div>
                                                    <div class="col-md-8">
                                                        @Html.DropDownListFor(m => m.DisposalCatId, Model.ListSelectDisposalCat, "--Select--", new
                                                        {
                                                            @Id = "DisposalCatId"@*, @name = "complaintUserControlDivision"*@,
@class = "form-control",
@data_val_tag = "T1",
@data_val_is_required = "True",
