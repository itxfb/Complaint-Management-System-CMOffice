<script type="text/javascript">
    var Model = @Html.Raw(Json.Encode(@Model));
    //var stepWiseDivIds = [{StepEditable:"Step0Editable",StepNonEditable:"Step0NonEditable"}];
    var currentStep = parseInt(Model.CurrentStep);

    $(document).ready(function () {
        debugger;


        RegisterLocalValidation("#SubmitComplaintAction", "T1");
        RegisterCloning("#SubmitComplaintAction", "T1");
        if(/*currentStep!=2*/ currentStep<=2 && Model.ListDbActionReportLogs.length==0) {
            $("#BtnAddProceeding").trigger('click');
        }
        PopulateEffectOfCurrentStep();
    });



    function PopulateEffectOfCurrentStep(){
        for (var i = 0; i < currentStep; i++) {
            IncrementValueOnButtonClick($("#CurrentDivIndex"), 1,4);
        }
    }


    /* ---------- add/remove proceedings ---------- */
    $("#BtnAddProceeding").on('click', function () {
        $('#BtnRemoveProceeding').prop('disabled', false);
    });

    $("#BtnRemoveProceeding").on('click', function () {
        var proceedings = $('.proceedingInfo:visible').length;

        if (proceedings => 1) {
            if (proceedings - 1 == 0)
                $('#BtnRemoveProceeding').prop('disabled', true);
            $(".proceedingInfo:visible")[proceedings - 1].remove();
        }
    });
    /* ---------- add/remove proceedings : end ---------- */

    $('.textDetail').on('change keydown keyup paste', function (e) {
        if (this.value != '') {
            var val = this.value.replace(/[^a-zA-Z0-9_\ ]/gi, '');
            $('input[name="' + e.target.name + '"]').val(val);
        }
    })

    $("#BtnShowNext").on('click', function() {
        debugger;
        IncrementValueOnButtonClick($("#CurrentDivIndex"), 1,4);
    });

    $("#BtnHideNext").on('click', function() {
        IncrementValueOnButtonClick($("#CurrentDivIndex"), -1,4);
    });

    $("#DisposalCatId").on('change', function() {
        //alert($(this).val()) ;
        var selectedVal = $(this).val();
